{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react-hooks", "eslint-plugin-promise"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "promise/catch-or-return": "error"
  }
}
